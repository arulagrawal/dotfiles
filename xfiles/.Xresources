#include ".cache/squash/x_colors"
!#include "/home/arul/.xrdb/colors/monodevek"
!#include "~/.xrdb/fonts/hack"
!#include "/home/arul/.xrdb/rofi/nord"
!#include ".config/rasi/config.rasi"

Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
!XTerm.vt100.faceName: Iosevka Term:size=11:antialias=true
XTerm.vt100.faceName: Hack Nerd Font:size=11:antialias=true
!XTerm.vt100.font: -schumacher-clean-medium-r-normal-*-12-120-75-75-c-*-iso8859-*
Xft.rgba: rgb
Xft.dpi: 96
XTerm.termName: xterm-256color
XTerm.vt100.locale: true
XTerm.vt100.selectToClipboard: true
XTerm.scaleHeight: 1.01

URxvt.scrollBar: false
URxvt*depth: 32
URxvt.perl-ext-common: default,matcher,resize-font
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.clipboard.autocopy: true
!URxvt.matcher.rend.0: Uline Bold fg5
URxvt*borderless: true
!URxvt*inheritPixMap: true
!URxvt*transparency: true
!URxvt*shading: 120
URxvt.letterSpace: -0.9
URxvt*background: [50]#282a36
*.foreground: #F8F8F2
*.color0:     #000000
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
