st.color6:     #8BE9FD
st.color14:    #9AEDFE
st.color7:     #BFBFBF
st.color15:    #E6E6E6


Xft.dpi: 190
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
URxvt*lineSpace: -1


URxvt*selection_to_clipboard: false
