!#include ".config/rasi/config.rasi"

!Xft.autohint:1
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
!XTerm.vt100.faceName: Iosevka Term:size=11:antialias=true
XTerm.vt100.faceName: Hack Nerd Font:pixelsize=15:antialias=true
!XTerm.vt100.font: -schumacher-clean-medium-r-normal-*-12-120-75-75-c-*-iso8859-*
Xft.rgba: rgb
Xft.dpi: 96
XTerm.termName: xterm-256color
XTerm.vt100.locale: true
XTerm.scaleHeight: 1.01
!XCursor.theme: Breeze Snow
*.background: #282a36
URxvt.scrollBar: false
URxvt*depth: 32
URxvt.perl-ext-common: default,matcher,resize-font
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.clipboard.autocopy: true
!URxvt.matcher.rend.0: Uline Bold fg5
URxvt*borderless: true
!URxvt*inheritPixMap: true
!URxvt*transparency: true
!URxvt*shading: 120
URxvt.letterSpace: -1
URxvt*background: [10]#282a36
URxvt.font:xft:Hack Nerd Font Mono:pixelsize=15:antialias=true:hinting=full, \
           xft:Material Icons:pixelsize=15:antialias=true
URxvt.boldFont:xft:Hack Nerd Font Mono:bold:pixelsize=15:antialias=true
!*.cursorColor:  #c4c4b5
*.foreground: #F8F8F2
*.color0:     #000000
